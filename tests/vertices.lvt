\documentclass[twocolumn]{article}
\IfFileExists{regression.sty}{\usepackage{regression}}{\usepackage{support/regression}}

\def\vertices{%
  dot,%
  square dot,%
  empty dot,%
  crossed dot,%
  blob%
}

\def\sizes{%
  small,%
  medium,%
  large%
}

\begin{document}

\section{Information}

\TeXTable
\clearpage

\foreach \vertex in \vertices {
  \section{\vertex\ (center)}

  \foreach \size in \sizes {
    \edef\name{\vertex\ (\size)}
    \subsection\name

    \expandafter\BEGINTEST\expandafter{\name}
    \feynmandiagram[\size, horizontal=a to b] {
      a -- b [\vertex]  -- {c, d},
    };
    \ENDTEST
  }
  \clearpage
}

\section{Particle (center)}

\foreach \size in \sizes {
  \edef\name{Particle (\size)}
  \subsection\name

  \expandafter\BEGINTEST\expandafter{\name}
  \feynmandiagram[\size, horizontal=a to b] {
    a -- b [particle=\(\mu\)]  -- {c, d},
  };
  \ENDTEST
}
\clearpage

\foreach \vertex in \vertices {
  \section{\vertex\ (edge)}

  \foreach \size in \sizes {
    \edef\name{\vertex\ (\size)}
    \subsection\name

    \expandafter\BEGINTEST\expandafter{\name}
    \feynmandiagram[\size, horizontal=a to b] {
      a [\vertex] -- b -- {c [\vertex], d [\vertex]},
    };
    \ENDTEST
  }
  \clearpage
}

\section{Particle (edge)}

\foreach \size in \sizes {
  \edef\name{Particle (\size)}
  \subsection\name

  \expandafter\BEGINTEST\expandafter{\name}
  \feynmandiagram[\size, horizontal=a to b] {
    a [particle=\(\mu\)] -- b -- {c [particle=\(\mu\)], d [particle=\(\mu\)]},
  };
  \ENDTEST
}

\end{document}
