\documentclass[twocolumn]{article}
\IfFileExists{regression.sty}{\usepackage{regression}}{\usepackage{support/regression}}

\def\edges{%
  plain,%
  boson,%
  charged boson,%
  anti charged boson,%
  photon,%
  scalar,%
  charged scalar,%
  anti charged scalar,%
  ghost,%
  fermion,%
  anti fermion,%
  majorana,%
  anti majorana,%
  gluon%
}

\def\sizes{%
  small,%
  medium,%
  large%
}

\begin{document}

\section{Information}

\TeXTable
\clearpage

\START

\foreach \edge in \edges {
  \section{\edge}

  \foreach \size in \sizes {
    \edef\name{\edge--\size}
    \subsection\name

    \expandafter\BEGINTEST\expandafter{\name}
    \feynmandiagram[\size, horizontal=a to b] {
      a -- [\edge] b,
    };
    \ENDTEST
  }

  \clearpage
}

\foreach \edgea [count=\i] in \edges {
  \section{\edgea}

  \foreach \edgeb [count=\j] in \edges {
    \edef\name{\edgea--\edgeb}
    \subsection\name

    \expandafter\BEGINTEST\expandafter{\name}
    \feynmandiagram[horizontal=a to b] {
      a -- [\edgea, \edgeb] b,
    };
    \ENDTEST
  }
  \clearpage
}

\end{document}
