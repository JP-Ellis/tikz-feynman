\documentclass[twocolumn]{article}
\IfFileExists{regression.sty}{\usepackage{regression}}{\usepackage{support/regression}}

\def\modifiers{%
  half left,%
  half right,%
  quarter left,%
  quarter right,%
  out=90,%
  in=90,%
  out=45,%
  in=135%
}

\def\momenta{%
  momentum,%
  momentum',%
  reversed momentum,%
  reversed momentum',%
  rmomentum,%
  rmomentum'%
}

\def\sizes{%
  small,%
  medium,%
  large%
}

\begin{document}

\section{Information}

\TeXTable
\clearpage

\foreach \momentum in \momenta {
  \section{\momentum}

  \foreach \size in \sizes {
    \edef\name{\momentum\ (\size)}
    \subsection\name

    \expandafter\BEGINTEST\expandafter{\name}
    \feynmandiagram[\size, horizontal=a to b] {
      a -- [\momentum=\(p\)] b,
    };
    \ENDTEST
  }
}
\clearpage

\foreach \modifier in \modifiers {
  \section{\modifier}

  \foreach \size in \sizes {
    \edef\name{\modifier\ (\size)}
    \subsection\name

    \expandafter\BEGINTEST\expandafter{\name}
    \feynmandiagram[\size, horizontal=a to b] {
      a -- [\modifier] b,
    };
    \ENDTEST
  }
}
\clearpage

\foreach \momentum in \momenta {
  \section{\momentum}

  \foreach \modifier in \modifiers {
    \edef\name{\momentum\ (\modifier)}
    \subsection\name

    \expandafter\BEGINTEST\expandafter{\name}
    \feynmandiagram[horizontal=a to b] {
      a -- [\momentum=\(p\), \modifier] b,
    };
    \ENDTEST
  }
}

